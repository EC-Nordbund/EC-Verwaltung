{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "strict": false,
    "jsx": "preserve",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "outDir": ".ts-tmp",
    "composite": true,
    "declarationMap": true,
    "sourceMap": true,
    "importHelpers": true,
    "baseUrl": "./",
    "rootDir": "./",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  }
}
